<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2E2E2E"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".InfoFragment">

    <ImageView
        android:id="@+id/meww"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/mew"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="50dp"
        android:layout_marginStart="30dp">
    </ImageView>
    <TextView
        android:id="@+id/name"
        android:fontFamily="@font/suitsemibold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="박선후"
        android:textColor="#ffffff"
        android:textSize="20sp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@+id/meww"
        android:layout_marginTop="72dp"
        android:layout_marginStart="22dp">
    </TextView>
    <TextView
        android:id="@+id/email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="tjsgn07@gmail.com"
        android:textColor="#ffffff"
        android:textSize="15sp"
        android:fontFamily="@font/suitregular"
        app:layout_constraintTop_toBottomOf="@+id/name"
        app:layout_constraintStart_toStartOf="@+id/name">
    </TextView>
    <FrameLayout
        android:id="@+id/framem"
        android:layout_width="match_parent"
        android:layout_height="12dp"
        android:background="#1E1E1E"
        app:layout_constraintTop_toBottomOf="@+id/email"
        android:layout_marginTop="36.5dp">
    </FrameLayout>
    <EditText
        android:id="@+id/searchl"
        android:layout_width="350dp"
        android:layout_height="46dp"
        android:hint="설정 검색하기"
        android:textColorHint="#5C5C5C"
        android:fontFamily="@font/suitregular"
        android:drawableLeft="@drawable/info_search"
        android:background="@drawable/edittext_design"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/framem"
        android:paddingStart="15dp"
        android:layout_marginTop="26dp">
    </EditText>
    <TextView
        android:id="@+id/record"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="기록"
        android:textSize="22sp"
        android:textColor="#BEBEBE"
        android:fontFamily="@font/suitsemibold"
        app:layout_constraintTop_toBottomOf="@+id/searchl"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="29dp"
        android:layout_marginStart="24dp">
    </TextView>
    <TextView
        android:id="@+id/record1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="대여, 소장"
        android:textSize="18sp"
        android:textColor="@color/white"
        android:fontFamily="@font/suitregular"
        app:layout_constraintTop_toBottomOf="@+id/record"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp">
    </TextView>
    <TextView
        android:id="@+id/record2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="다운로드한 오디오"
        android:textSize="18sp"
        android:textColor="@color/white"
        android:fontFamily="@font/suitregular"
        app:layout_constraintTop_toBottomOf="@+id/record1"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp">
    </TextView>
    <TextView
        android:id="@+id/record3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="최근 청취 기록"
        android:textSize="18sp"
        android:textColor="@color/white"
        android:fontFamily="@font/suitregular"
        app:layout_constraintTop_toBottomOf="@+id/record2"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp">
    </TextView>
    <FrameLayout
        android:id="@+id/underre"
        android:layout_width="354dp"
        android:layout_height="1dp"
        android:background="#606060"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/record3"
        android:layout_marginTop="15dp">
    </FrameLayout>

    <TextView
        android:id="@+id/pcenter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="고객센터"
        android:textSize="22sp"
        android:textColor="#BEBEBE"
        android:fontFamily="@font/suitsemibold"
        app:layout_constraintTop_toBottomOf="@+id/underre"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="20dp"
        android:layout_marginStart="24dp">
    </TextView>
    <TextView
        android:id="@+id/pcenter1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1:1 문의 내역"
        android:textSize="18sp"
        android:textColor="@color/white"
        android:fontFamily="@font/suitregular"
        app:layout_constraintTop_toBottomOf="@+id/pcenter"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp">
    </TextView>
    <TextView
        android:id="@+id/pcenter2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="자주 묻는 질문"
        android:textSize="18sp"
        android:textColor="@color/white"
        android:fontFamily="@font/suitregular"
        app:layout_constraintTop_toBottomOf="@+id/pcenter1"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp">
    </TextView>
    <FrameLayout
        android:id="@+id/underrer"
        android:layout_width="354dp"
        android:layout_height="1dp"
        android:background="#606060"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pcenter2"
        android:layout_marginTop="15dp">
    </FrameLayout>

    <TextView
        android:id="@+id/setting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="고객센터"
        android:textSize="22sp"
        android:textColor="#BEBEBE"
        android:fontFamily="@font/suitsemibold"
        app:layout_constraintTop_toBottomOf="@+id/underrer"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="20dp"
        android:layout_marginStart="24dp">
    </TextView>
    <TextView
        android:id="@+id/setting1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="개인정보 관리"
        android:textSize="18sp"
        android:textColor="@color/white"
        android:fontFamily="@font/suitregular"
        app:layout_constraintTop_toBottomOf="@+id/setting"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp">
    </TextView>
    <TextView
        android:id="@+id/setting2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="언어 설정"
        android:textSize="18sp"
        android:textColor="@color/white"
        android:fontFamily="@font/suitregular"
        app:layout_constraintTop_toBottomOf="@+id/setting1"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp">
    </TextView>
    <TextView
        android:id="@+id/setting3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="알림"
        android:textSize="18sp"
        android:textColor="@color/white"
        android:fontFamily="@font/suitregular"
        app:layout_constraintTop_toBottomOf="@+id/setting2"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp">
    </TextView>
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/info_arrow"
        android:background="#2E2E2E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginEnd="41dp"
        android:layout_marginTop="311dp">
    </ImageButton>
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/info_arrow"
        android:background="#2E2E2E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginEnd="41dp"
        android:layout_marginTop="348dp">
    </ImageButton>
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/info_arrow"
        android:background="#2E2E2E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginEnd="41dp"
        android:layout_marginTop="386dp">
    </ImageButton>
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/info_arrow"
        android:background="#2E2E2E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/underre"
        android:layout_marginEnd="41dp"
        android:layout_marginTop="63dp">
    </ImageButton>
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/info_arrow"
        android:background="#2E2E2E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/underre"
        android:layout_marginEnd="41dp"
        android:layout_marginTop="104dp">
    </ImageButton>
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/info_arrow"
        android:background="#2E2E2E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/underrer"
        android:layout_marginEnd="41dp"
        android:layout_marginTop="65dp">
    </ImageButton>
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/info_arrow"
        android:background="#2E2E2E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/underrer"
        android:layout_marginEnd="41dp"
        android:layout_marginTop="106dp">
    </ImageButton>
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/info_arrow"
        android:background="#2E2E2E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/underrer"
        android:layout_marginEnd="41dp"
        android:layout_marginTop="146dp">
    </ImageButton>
</androidx.constraintlayout.widget.ConstraintLayout>